{
  "name": "paperplane-api",
  "version": "0.0.1",
  "description": "",
  "author": "jia-niang <1@paperplane.cc>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn start:dev",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dbgen": "prisma generate",
    "dbpush": "dotenv -c development -- prisma db push",
    "dbmi": "dotenv -c development -- prisma migrate dev",
    "dbdeploy": "dotenv -c development -- prisma migrate deploy",
    "dbdeploy:prod": "dotenv -c production -- prisma migrate deploy"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.552.0",
    "@azure/openai": "^2.0.0-beta.2",
    "@larksuiteoapi/node-sdk": "^1.23.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/serve-static": "^3.0.1",
    "@prisma/client": "^5.6.0",
    "axios": "^1.3.4",
    "axios-cache-interceptor": "^1.5.1",
    "axios-retry": "^4.1.0",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "dayjs": "^1.11.7",
    "dingtalk-robot-sender": "^1.2.0",
    "dotenv-cli": "^7.3.0",
    "ejs": "^3.1.9",
    "express-session": "^1.18.0",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "mime": "^3",
    "nestjs-prisma": "^0.22.0",
    "nestjs-session": "^3.0.1",
    "object-code": "^1.3.3",
    "openai": "^4.66.1",
    "passport": "^0.7.0",
    "prisma-soft-delete-middleware": "^1.2.0",
    "puppeteer": "^19.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "simple-git": "^3.17.0",
    "sshpk": "^1.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.3",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/dingtalk-robot-sender": "^1.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.18.0",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/sshpk": "^1.17.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.3.1",
    "prettier": "^3.0.2",
    "prisma": "^5.6.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
